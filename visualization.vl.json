{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "History of Mean Income in USA",
  "width": 550,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/mtur0005/Visualisation2/main/gender_year_income.csv"
  },
  "mark": {"type": "line", "point": true},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "title": "Year",
      "axis": {"tickCount": 5}
    },
    "y": {
      "field": "Total_Mean",
      "type": "quantitative",
      "title": "Income (USD)",
      "axis": {"tickCount": 5}
    },
    "tooltip": [
      {"field": "Year", "type": "temporal", "timeUnit": "year"},
      {
        "field": "Total_Mean",
        "type": "quantitative",
        "title": "Mean Income (USD)"
      }
    ]
  },
  "config": {"axis": {"grid": false}}
}